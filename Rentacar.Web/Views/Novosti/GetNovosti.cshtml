@using Rentacar.Web.ViewModels
@{
    ViewData["Title"] = "GetNovosti";
    var ulazniModel = (NovostiGetVM)ViewData["ulazniModel"];
    //var lista2 = (List<Notifikacije>)ViewBag.NovostiVB;
}

<h2>GetNovosti</h2>
@foreach (var x in ulazniModel.rows)
{
    <h3>@x.Naslov</h3>
    <p>@Html.Raw(@x.Sadrzaj)</p>
    <div>@x.Datum_i_vrijeme_objave</div>
}
